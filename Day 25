#include<iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;//problems
        int x;//marks for each problem
        int y;//marks that chef wants
        int count = 0;
        cin>>n>>x>>y;
        for(int i=0;i<=n;i++)
        {
            if(i*x==y)
            {
                count++;
                cout<<"Yes"<<endl;
            }
        }
         if(count==0)
            {
                cout<<"No"<<endl;
            }
    }
    return 0;
}
