#include <iostream>
using namespace std;

int main() {
int t;
cin>>t;
while(t--)
{
    int n;//number of candies
    int m;//number of friends
    cin>>n>>m;
    if((n/m)%2==0 && n%m==0)
    {
        cout<<"Yes"<<endl;
    }
    else if(n%m!=0)
    {
        cout<<"No"<<endl;
    }
    else
    {
        cout<<"No"<<endl;
    }
}
	return 0;
}
