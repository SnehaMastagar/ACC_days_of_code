#include<iostream>
#include<algorithm>
using namespace std;

#define MAX(a,b) (a>b)?a:b
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;
        cin>>n;
        int arr[n];
        int max=0;
        for(int i=0;i<n;i++)
        {
            cin>>arr[i];
            max=MAX(max,arr[i]);
        }
        cout<<max<<endl;
    }
    return 0;
}
