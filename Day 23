#include <iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;
    
    for(int i=0; i<t; i++)
    {
        int A,B,C,X;
        cin>>A>>B>>C>>X;
        
        if(A+B>=X || B+C>=X || C+A>=X)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
}
