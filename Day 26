#include<iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        int n;//number of people in Chefland
        int x;//minimum age required to vote in Chefland
        int age;//age of person
        int count=0;
        cin>>n>>x;
        for(int i=0;i<n;i++)
        {
            cin>>age;
            if(age>=x)
            {
                count++;
            }
        }
        cout<<count<<endl;
    }
    return 0;
}
