#include <iostream>
using namespace std;

int main() 
{
	int t;
	cin>>t;
	
	for(int i=1;i<=t;i++)
	{
	    int A,B,C,E;
	    int D=15;
	    cin>>A>>B>>C>>D>>E;
	    
	    if(A+B <=D && C<=E )
	    {
	        cout<<"YES"<<endl;
	    }
	    else if(A+C<=D && B<=E)
	    {
	        cout<<"YES"<<endl;
	    }
	    else if(B+C<=D && A<=E)
	    {
	        cout<<"YES"<<endl;
	    }
	    else {
	        cout<<"NO"<<endl;
	    }
	}
	return 0;
}
