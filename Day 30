#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int x;//plates chef prepared
	    int y;//neighours ate the plate
	    cin>>x>>y;
	    if(x/y>=20)
	    {
	        cout<<"20"<<endl;
	    }
	    else
	    {
	        cout<<x/y<<endl;
	    }
	    }
	
	return 0;
}
